        }
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/runtime": "^7.9.2",
        "@react-native-community/eslint-config": "^1.1.0",
        "@types/react": "^16.9.34",
        "@types/react-native": "^0.62.2",
        "@types/react-redux": "^7.1.7",
        "@typescript-eslint/eslint-plugin": "^2.28.0",
        "@typescript-eslint/parser": "^2.28.0",
        "babel-jest": "^25.3.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "del-cli": "^3.0.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.19.0",
        "jest": "^25.3.0",
        "metro-react-native-babel-preset": "^0.58.0",
        "prettier": "^2.0.4",
        "react-test-renderer": "16.12.0",
        "redux-devtools-extension": "^2.13.8",
        "typescript": "^3.8.3"
    },
    "scripts": {
        "dev:watch": "del-cli build/dist/ && tsc && npm run pre-publish:dev-mode && tsc -w",
        "dev:start": "npm run lint && react-native start --reset-cache",
        "start:android": "react-native run-android",
        "bundle-ios": "react-native bundle --entry-file='./build/dist/ReactNative/src/index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
        "bundle-ios-dev": "react-native bundle --entry-file='./build/dist/ReactNative/src/index.js' --bundle-output='./ios/main.jsbundle' --dev=true --platform='ios' --assets-dest='./ios'",
        "bundle-android": "react-native bundle --platform android --dev false --entry-file ./build/dist/ReactNative/src/index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/",
        "bundle-android-dev": "react-native bundle --entry-file='./build/dist/ReactNative/src/index.js' --bundle-output='./android/main.jsbundle' --dev=true --platform='android' --assets-dest='./android'",
        "build": "echo 'build' && del-cli build && echo 'ran del-cli build' && tsc --extendedDiagnostics && echo 'ran tsc' && npm run pre-publish:dev-mode && echo 'ran npm run pre-publish:dev-mode'",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "publish:android": "npm run build && npm run pre-publish:android && cd android && ./gradlew clean && ./gradlew --stop && ./gradlew publishApkrelease && cd ../",
        "pre-publish:ios": "npm run bundle-ios && cd ./scripts && node ./updateVersionJson.js ios && cd ../",
        "pre-publish:android": "npm run bundle-android && cd ./scripts && node ./updateVersionJson.js android && cd ../",
        "pre-publish:dev-mode": "cd ./scripts && node ./updateVersionJson.js dev-mode && cd ../",
        "install-clean": "npm run _watchmen-reset && npm run _node_modules-reset && npm run _pod-reset && npm run _gradlew-reset",
        "_watchmen-reset": "echo 'watchman watch-del-all'",
        "_node_modules-reset": "rm -rf ./node_modules && npm cache verify && npm install",
        "_pod-reset": "rm -r ~/Library/Developer/Xcode/DerivedData ; cd ./ios && pod deintegrate && pod repo update && pod install ; cd ..",
        "_gradlew-reset": "cd ./android && ./gradlew clean && ./gradlew --stop; cd .."
    },
    "jest": {
        "preset": "react-native"
    }
}
